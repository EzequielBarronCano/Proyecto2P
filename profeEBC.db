create database profeEBC;
use profeEBC;

create table alumno(
    n_control int primary key auto_increment,
    nom_al varchar(50) not null default 'NOMBRE',
    ap_paterno varchar(100) not null default 'AP_PATERNO',
    ap_materno varchar(100) not null default 'AP_MATERNO',
    curp varchar(18) not null default 'XAXA500101HQTRNZ01',
    f_nac_al date not null default'1900-01-01',
    n_tel_al bigint(10) not null default'9876543210',
    e_mail varchar(30) not null default 'alumno@email.com',
    calle varchar(50),
    num varchar(5),
    colonia varchar(50),
    municipio varchar(50),
    estado varchar(50)
);

